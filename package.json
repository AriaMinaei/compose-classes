    ],
    "globals": {},
    "testURL": "http://localhost"
  },
  "dependencies": {
    "lodash-es": "^4.17.11"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/lodash-es": "^4.17.1",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.1",
    "typescript": "^3.0.3"
  }
}
