{
  "name": "compose-css-classes",
  "version": "1.0.2",
  "description": "A small utility function to make react components that use css modules to have extensible styles.",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc ",
    "prepublish": "npm run build"
  },
  "keywords": [],
  "author": "Aria Minaei aria.minaei@gmail.com",
  "license": "MIT",
  "jest": {
    "testMatch": [
      "**/?(*.)(test).ts?(x)"
    ],
    "moduleNameMapper": {
      "lodash-es": "lodash"
    },
    "automock": false,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
